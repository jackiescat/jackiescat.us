{
  "private": true,
  "name": "jackiescat.us",
  "version": "0.0.1",
  "scripts": {
    "deploy": "gh-pages-deploy",
    "build": "gulp dist",
    "cleansrc": "rm -rf README.md package.json src node_modules gulpfile.js .travis.yml"
  },
  "gh-pages-deploy": {
    "staticpath": "dist",
    "cname": "jackiescat.us",
    "prep": [
      "build"
    ],
    "post": [
      "cleansrc"
    ],
    "noprompt": true
  },
  "dependencies": {
    "gh-pages-deploy": "0.4.2"
  },
  "devDependencies": {
    "browser-sync": "^2.13.0",
    "del": "^2.2.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-cache": "^0.4.5",
    "gulp-cssmin": "^0.1.7",
    "gulp-imagemin": "^3.0.1",
    "gulp-sass": "^2.3.2",
    "gulp-uglify": "^1.5.3",
    "imagemin": "^5.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackiescat/jackiescat.us.git"
  },
  "bugs": {
    "url": "https://github.com/jackiescat/jackiescat.us/issues"
  },
  "homepage": "https://github.com/jackiescat/jackiescat.us#readme"
}
